open sys

class BankAccount {
	var balance = 0
	func deposit(amount Int) {
		if amount > 0 {
			balance += amount
		}
	}
	func withdraw(amount Int) Int {
		if 0 < amount <= balance {
			balance -= amount
			return balance
		}
		else {
			throw "insufficient funds"
		}
	}
}

let myAccount = BankAccount()
myAccount.deposit(50)
myAccount.withdraw(20)